    <rdfs:subClassOf>
        <rdfs:Class rdf:about="http://www.w3.org/2000/10/swap/pim/contact#Person" rdfs:label="Person">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
