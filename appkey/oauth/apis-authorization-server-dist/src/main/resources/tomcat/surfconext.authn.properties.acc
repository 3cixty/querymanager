entityId=https\://apis.acc.surfconext.nl
assertionConsumerURI=https\://apis.acc.surfconext.nl/oauth2/authorize
idpUrl=https\://engine.acc.surfconext.nl/authentication/idp/single-sign-on

idpEntityId=https\://engine.acc.surfconext.nl/authentication/idp/metadata
spPrivateKey=??
spCertificate=??

# When using SURFconext, this should be: urn:oid:1.3.6.1.4.1.1076.20.40.40.1
samlUuidAttribute=urn:oid:1.3.6.1.4.1.1076.20.40.40.1

# The group where one need to be a member of for authorization in the JavaScript client apis admin
admin.client.apis.teamname=urn\:collab\:group\:dev.surfteams.nl\:nl\:surfnet\:management\:managementvo

openConextApiClient=nl.surfnet.coin.api.client.OpenConextOAuthClientImpl

# Set this to true for groep enrichment and to false for not making the call to the group api
api-enrich-principal=true

# Optional properties only to be used when api-enrich-pricipal is set to true
api-baseurl=https\://api.acc.surfconext.nl/v1/
api-consumerkey=https\://apis.acc.surfconext.nl
api-consumersecret=???
api-callbackuri=https\://apis.acc.surfconext.nl/oauth2/authorize?apiOauthCallback\=true
